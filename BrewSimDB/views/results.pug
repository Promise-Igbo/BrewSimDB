extends layout

block content
    h3.text-center - Homebrewers DB Query Results -
    br
    table#table
        thead
            tr
                th
                    
        tbody
            if print.length>0
            each item, i in print
                tr
                    to
                        each attribute, j in item
                            = j + ": " + attribute + "  |  "
                        br